@charset "utf-8";

/* CSS Document */
html,body,div,span,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,address,big,cite,code,del,em,font,img,ins,small,strong,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend
	{
	margin: 0;
	padding: 0;
}

ol,ul {
	list-style: none;
}

:focus {
	outline: 0;
}

a img {
	border: 0;
}

img {
	vertical-align: middle;
}

table {
	empty-cells: show;
}

ins {
	text-decoration: none;
}

del {
	text-decoration: line-through;
}

h1 {
	font-size: 20px;
	font-family: Arial, Lucida, Verdana, "microsoft yahei", "宋体", Helvetica,
		sans-serif;
}

h2 {
	font-size: 14px;
}

h3 {
	font-size: 14px;
}

h4 {
	font-size: 12px;
}

h5 {
	font-size: 12px;
}

h6 {
	font-size: 12px;
}

a:link,a:visited {
	color: #000;
	text-decoration: none;
}

a:hover,a:active,a:focus {
	text-decoration: underline;
	color: #F00;
}

body {
	margin: 0 auto;
}

.container {
	width: 990px;
	padding: 0;
	margin: 10px auto;
	zoom: 1;
}

.w {
	width: 990px;
	padding: 0;
	margin: 10px auto;
	zoom: 1;
}

.w1 {
	width: 890px;
	margin: 0 auto;
}

#logo {
	padding: 10px 0;
}

.logo-bg {
	padding-left: 30px;
	font-size: 24px;
	font-family: arial, "宋体";
	font-style: #ffffff;
}

.entry-bg {
	background: #e8e8e8;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	padding: 3px;
	margin-bottom: 150px;
}

#entry {
	background: #e8e8e8;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	padding: 5px;
	margin: 60px auto 55px;
	position: relative;
}

#entry .mc {
	border: 1px solid #e1e1e1;
	background: #ffffff;
	height: 354px;
}

#entry .form {
	position: absolute;
	top: 40px;
	right: 30px;
	width: 320px;
	overflow: hidden;
	height: 354px;
}

#entry .text {
	float: none;
	width: 250px;
	height: 24px;
	line-height: 24px;
	padding: 4px 20px 4px 3px;
	border: 1px solid #cccccc;
	font-size: 14px;
	font-family: arial, "宋体";
}

#entry .label {
	width: 100px;
	text-align: left;
	font-size: 14px;
	margin: 10px 10px 10px 0;
}

#entry .check {
	margin: 10px;
}

#entry .handcursor {
	cursor: pointer;
}

.form label,.form input,.form select,.form textarea,.form button,.form .label
	{
	float: left;
	font-size: 12px;
}

.tip {
	color: red;
	margin-left: 5px;
}

#regist {
	background: #e8e8e8;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	padding: 5px;
	margin: 20px auto 55px;
	position: relative;
}

#regist .mc {
	border: 1px solid #e1e1e1;
	background: #ffffff;
	height: 500px;
}

#regist .form {
	float: left;
	width: 750px;
	position: relative;
	top: 20px;
}

#regist .item {
	padding-top: 9px;
	height: 55px;
	line-height: 34px;
	position: relative;
	z-index: 1;
	float: left;
	margin-left: 10px;
}

#regist .label {
	width: 190px;
	text-align: right;
	font-size: 14px;
	padding-right: 10px;
}

#regist .label b {
	font-family: "????";
	margin-right: 5px;
