}

.tit {
	float: left;
	line-height: 32px;
	font-size: 20px;
	font-family: "microsoft yahei";
	margin-right: 15px;
	float: left;
	width: 100%;
	color: #333;
	padding: 10px 0;
}

.tit a {
	color: #333;
}

.tit a:hover {
	color: #1FD1B7;
	text-decoration: none;
}

.sidebar1 {
	background: #1FD1B7;
