}

ul.nav {
	list-style: none;
	margin-bottom: 15px;
}

.nav .label {
	font-size: 18px;
	font-family: arial, "宋体";
	line-height: 44px;
	color: #42413C;
	padding-top: 20px;
}

ul.nav li {
	font-size: 16px;
	font-family: arial, "宋体";
	line-height: 24px;
}

ul.nav a,ul.nav a:visited {
	padding: 5px 5px 5px 15px;
	display: block;
	width: 100px;
	text-decoration: none;
	color: #42413C;
}

ul.nav a:link {
	color: #42413C;
}

ul.nav a:hover,ul.nav a:active,ul.nav a:focus {
	color: #FFF;
}

